<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>


<DialogPane xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="se.backede.budget.LoginController">
   <content>
      <VBox alignment="CENTER" prefHeight="155.0" prefWidth="332.0" spacing="20.0">
          <children>
              <TextField fx:id="username" promptText="username">
                  <VBox.margin>
                      <Insets left="25.0" right="25.0" />
                  </VBox.margin>
              </TextField>
              <PasswordField fx:id="password" prefHeight="25.0" prefWidth="411.0" promptText="password">
                  <VBox.margin>
                      <Insets left="25.0" right="25.0" />
                  </VBox.margin>
              </PasswordField>
              <Button fx:id="primaryButton" onAction="#login" text="Login" />
          </children>
          <padding>
              <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
          </padding>
      </VBox>
   </content>
</DialogPane>
